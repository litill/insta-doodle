{
	"name": "polco-insta-doodle",
	"version": "0.1.0",
	"description": "Example Polcode Gutenberg-Block plugin allowing you to create instant doodles. Written with ESNext standard and JSX support – build step required.",
	"author": "Grzegorz Jasiński",
	"license": "GPL-2.0-or-later",
	"main": "build/index.js",
	"scripts": {
		"build": "wp-scripts build src/index.js src/front.js --output-path=build",
		"format:js": "wp-scripts format-js",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"start": "wp-scripts start src/index.js src/front.js --output-path=build",
		"packages-update": "wp-scripts packages-update"
	},
	"dependencies": {
		"@wordpress/block-editor": "^5.3.1",
		"@wordpress/blocks": "^8.0.1",
		"@wordpress/i18n": "^3.19.1",
		"lit-element": "^2.4.0",
		"p5": "^1.3.1",
		"react-color": "^2.19.3",
		"react-color-palette": "^4.0.0",
		"react-colorful": "^5.1.2"
	},
	"devDependencies": {
		"@wordpress/scripts": "^14.1.0"
	}
}
